# Test AdamWPrune vs Adam with state/movement pruning on ResNet-50
# This configuration runs tests comparing pruning methods

# Model selection
CONFIG_MODEL_RESNET50=y

# Dataset configuration (using CIFAR-100 for faster testing)
CONFIG_RESNET50_DATASET="cifar100"
CONFIG_RESNET50_DATA_DIR="./data"

# Training configuration
CONFIG_RESNET50_BATCH_SIZE=128
CONFIG_RESNET50_EPOCHS=100
CONFIG_RESNET50_NUM_WORKERS=4
CONFIG_RESNET50_LOG_INTERVAL=10
CONFIG_RESNET50_SAVE_MODEL=y
CONFIG_RESNET50_MONITOR_GPU=y

# Pruning configuration
CONFIG_RESNET50_PRUNING_START_EPOCH=10
CONFIG_RESNET50_PRUNING_END_EPOCH=80

# Test matrix configuration
CONFIG_TEST_MATRIX=y
CONFIG_TEST_MATRIX_NAME="resnet50_adam_vs_adamwprune_pruning"

# Test combinations
CONFIG_TEST_ADAM_BASELINE=y
CONFIG_TEST_ADAM_MOVEMENT=y
CONFIG_TEST_ADAMWPRUNE_BASELINE=y  
CONFIG_TEST_ADAMWPRUNE_STATE=y

# Sparsity level to test
CONFIG_TEST_SPARSITY_70=y

# Learning rates optimized for each optimizer
CONFIG_ADAM_LEARNING_RATE="0.001"
CONFIG_ADAMWPRUNE_LEARNING_RATE="0.001"